    <!-- Safe -->
    <string name="title_activity_safe">Safe</string>
    <string name="safe_dialog_first_title">Benvenuto in Safe</string>
    <string name="safe_dialog_first_content">Devi impostare una password per accedere a questa sezione. Assicurati di ricordarti la password o non potrai pi√π visualizzare i tuoi dati!</string>
