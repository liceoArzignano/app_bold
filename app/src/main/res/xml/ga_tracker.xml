<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="UnusedResources">
    <!-- End current session if app sleeps for a period of time -->
    <integer name="ga_sessionTimeout">300</integer>

    <!-- Enable automatic Activity measurement -->
    <bool name="ga_autoActivityTracking">true</bool>

    <!--  The property id associated with this analytics tracker -->
    <string name="ga_trackingId">UA-75642808-1</string>

    <string name="ga_sampleFrequency">100.0</string>

    <bool name="ga_reportUncaughtExceptions">true</bool>

    <string name="ga_anonymizeIp">true</string>

    <screenName name="it.liceoarzignano.bold.MainActivity">MainActivity</screenName>
    <screenName name="it.liceoarzignano.bold.marks.MarkListActivity">MarkListActivity</screenName>
    <screenName name="it.liceoarzignano.bold.ManagerActivity">MarkManagerActivity</screenName>
    <screenName name="it.liceoarzignano.bold.events.EventListActivity">EventListActivity</screenName>
    <screenName name="it.liceoarzignano.bold.safe.SafeActivity">SafeActivity</screenName>
    <screenName name="it.liceoarzignano.bold.IntroActivity">IntroActivity</screenName>
    <screenName name="it.liceoarzignano.bold.settings.SettingsActivity">SettingsActivity</screenName>
</resources>
